def getTotalX(a, b):
    a.sort()
    b.sort()
    c=[]
    e=[]
    for i in range(a[0],b[0]+1):
        m=0
        for j in (a):
            if i%j!=0:
                m+=1
                break
        if m==0:
            c.append(i)
    for i in c:
        m=0
        for j in (b):
            if j%i!=0:
                m+=1
                break
        if m==0:
            e.append(i)
    return len(e)
